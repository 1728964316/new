#include<stdio.h>
int main()
{
    int a = 1;
    int b = 1;
    while (a <= 9)
    {
        while (b <= 9)
        {
            if (b != 9)
            {
                printf("%d*%d=%d ", a, b, a * b);
                b += 1;
            }
            else
            {
                printf("%d*%d=%d\n", a, b, a * b);
                b = 1;
                break;
            }
        }
        a += 1;
    }
    return 0;
}
